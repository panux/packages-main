packages:
  fixesproto:
    dependencies:
    - xextproto
    - pkgconf
version: 5.0
builddependencies:
sources:
- https://www.x.org/releases/individual/proto/fixesproto-{{.Version}}.tar.bz2
script:
- tar -xf src/fixesproto-{{.Version}}.tar.bz2
- mv fixesproto-{{.Version}} fixesproto
- (cd fixesproto && ./configure --prefix=/usr)
- $(MAKE) -C fixesproto
- (cd fixesproto && make -j1 DESTDIR=$(shell pwd)/out/fixesproto install)
